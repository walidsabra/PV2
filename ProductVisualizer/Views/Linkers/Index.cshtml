@model IEnumerable<ProductVisualizer.Models.Linker>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SoftwareId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ObjectId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ObjectType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.comments)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SoftwareId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ObjectId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ObjectType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.comments)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
